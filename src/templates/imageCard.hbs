{{#each this}}
<a href='{{webformatURL}}' class="photo-link">
  <div class="photo-card">
  <img src='{{largeImageURL}}' alt={{tags}} loading="lazy" width="250"/>
  <div class="info">
    <p class="info-item">
      <b>{{likes}}Likes</b>
    </p>
    <p class="info-item">
      <b>{{views}}Views</b>
    </p>
    <p class="info-item">
      <b>{{comments}}Comments</b>
    </p>
    <p class="info-item">
      <b>{{downloads}}Downloads</b>
    </p>
  </div>
</div>
</a>
{{/each}}
