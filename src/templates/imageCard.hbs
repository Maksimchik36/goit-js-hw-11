{{#each this}}
<a href='{{webformatURL}}' style="height: 600px;">
  <div class="photo-card">
  <img src='{{largeImageURL}}' alt={{tags}} loading="lazy" />
  <div class="info" style="display: flex;">
    <p class="info-item">
      <b>{{likes}}Likes</b>
    </p>
    <p class="info-item">
      <b>{{views}}Views</b>
    </p>
    <p class="info-item">
      <b>{{comments}}Comments</b>
    </p>
    <p class="info-item">
      <b>{{downloads}}Downloads</b>
    </p>
  </div>
</div>
</a>
{{/each}}
